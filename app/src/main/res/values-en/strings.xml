    <!-- Headings -->
    <string name="headings_title_section">Headings</string>
    <string name="headings_abstract">Screens displaying dense text or information can be challenging to navigate for users with visual impairments.
        \n\nImplementing clear heading indicators allows for faster navigation through the content using swipe gestures.</string>

    <string name="headings_requirement_section_principle">Clearly separate information into sections.</string>
    <string name="headings_requirement_avoid_in_list">Avoid adding headings within lists.</string>

    <string name="headings_implementation_example_one">Heading 1</string>
    <string name="headings_implementation_example_two">Heading 2</string>

    <!-- Toggleables -->
    <string name="toggleables_title_section">Toggleables</string>
    <string name="toggleables_abstract">Toggleables are screen elements that allows a user to switch between two distinct states, typically "on" and "off" or "selected" and "unselected."
        \nThis category primarily encompasses components like Switches and CheckBoxes, where the user can easily toggle their state with a simple interaction.</string>

    <string name="toggleables_requirement_single_focus">Use a single group focus for the elements associated with the switch.</string>
    <string name="toggleables_requirement_semantic_context">to use semantics to give a clear context for the action of the switch.</string>

